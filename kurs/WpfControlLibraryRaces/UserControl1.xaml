<UserControl x:Class="WpfControlLibraryRaces.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfControlLibraryRaces"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="50" x:Name="diagram_races">
    <Canvas>
        <Rectangle  Stroke="White" Fill="{Binding ElementName=diagram_races, Path=WFill }" Width="50" Height="{Binding ElementName=diagram_races, Path=WHeight}">
            
        </Rectangle>
    </Canvas>
</UserControl>
